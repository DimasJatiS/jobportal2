                                                },
                                                "description": {
                                                    "type": "string",
                                                    "example": "Job description"
                                                },
                                                "salary": {
                                                    "type": "integer",
                                                    "example": 80000
                                                },
                                                "created_at": {
                                                    "type": "string",
                                                    "example": "2024-01-01T00:00:00Z"
                                                },
                                                "updated_at": {
                                                    "type": "string",
                                                    "example": "2024-01-01T00:00:00Z"
                                                }
                                            },
                                            "type": "object"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
