                            "application/json": {
                                "schema": {
                                    "properties": {
                                        "message": {
                                            "type": "string",
                                            "example": "Job created successfully"
                                        },
                                        "job": {
                                            "properties": {
                                                "id": {
                                                    "type": "integer",
                                                    "example": 1
                                                },
                                                "title": {
                                                    "type": "string",
                                                    "example": "Software Engineer"
                                                },
                                                "company": {
                                                    "type": "string",
                                                    "example": "Tech Corp"
                                                },
                                                "location": {
